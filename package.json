{
  "name": "amm-volume-bot",
  "version": "1.0.0",
  "description": "Automated trading bot to generate daily trading volume on DEX.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lem0nTree/AMM-Volume-Bot.git"
  },
  "keywords": [
    "AMM",
    "bot",
    "volume",
    "Uniswap",
    "PancakeSwap",
    "ERC20"
  ],
  "author": "Lem0nTree",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lem0nTree/AMM-Volume-Bot/issues"
  },
  "homepage": "https://github.com/Lem0nTree/AMM-Volume-Bot#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "eslint": "^8.52.0",
    "ethers": "^6.13.4",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "figlet": "^1.7.0",
    "gaussian": "^1.3.0",
    "node-schedule": "^2.1.1",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^6.9.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverageFrom": [
      "utils/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
